{
  "name": "simplebar-angular",
  "version": "2.5.0",
  "description": "Angular component for SimpleBar",
  "publishConfig": {
    "directory": "dist"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "keywords": [
    "simplebar-angular",
    "angular",
    "simplebar"
  ],
  "dependencies": {
    "simplebar": "^5.3.9",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": "^13.3.9",
    "@angular/core": "^13.3.9"
  },
  "module": "fesm2015/simplebar-angular.mjs",
  "es2020": "fesm2020/simplebar-angular.mjs",
  "esm2020": "esm2020/simplebar-angular.mjs",
  "fesm2020": "fesm2020/simplebar-angular.mjs",
  "fesm2015": "fesm2015/simplebar-angular.mjs",
  "typings": "simplebar-angular.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./simplebar-angular.d.ts",
      "esm2020": "./esm2020/simplebar-angular.mjs",
      "es2020": "./fesm2020/simplebar-angular.mjs",
      "es2015": "./fesm2015/simplebar-angular.mjs",
      "node": "./fesm2015/simplebar-angular.mjs",
      "default": "./fesm2020/simplebar-angular.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "node --eval \"console.error('ERROR: Trying to publish a package that has been compiled by Ivy in full compilation mode. This is not allowed.\\nPlease delete and rebuild the package with Ivy partial compilation mode, before attempting to publish.\\n')\" && exit 1"
  }
}